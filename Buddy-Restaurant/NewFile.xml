<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
                             http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">

    <welcome-file-list>
        <welcome-file>json-index.html</welcome-file>
    </welcome-file-list>
    <servlet>
        <servlet-name>jsonPlaceOrderServlet</servlet-name>
        <servlet-class>servletsss.jsonPlaceOrderServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>jsonPlaceOrderServlet</servlet-name>
        <url-pattern>/jsonPlaceOrderServlet</url-pattern>
    </servlet-mapping>

  
    <servlet>
        <servlet-name>jsonGetOrderByNameServlet</servlet-name>
        <servlet-class>servletsss.jsonGetOrderByNameServlet</servlet-class>
        <load-on-startup>1</load-on-startup>
    </servlet>
    <servlet-mapping>
        <servlet-name>jsonGetOrderByNameServlet</servlet-name>
        <url-pattern>/jsonGetOrderByNameServlet</url-pattern>
    </servlet-mapping>

   

</web-app>
